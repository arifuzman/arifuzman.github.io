#include <stdio.h>
#include <math.h>


int computeHomeValue(int popularity, int size) 
{
    return (pow(popularity, 3) + pow(size, 2)) * 10000;
}

int main() 
{
    int popularity, size;
    
printf("Enter popularity: "); 
scanf("%d", &popularity);

printf("Enter size: ");
scanf("%d", &size);

 int homeValue = computeHomeValue(popularity, size);
 printf("Home value is: %d\n", homeValue);

    return 0;
}
