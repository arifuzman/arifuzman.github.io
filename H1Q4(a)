#include <stdio.h>
#include <math.h>

void computeHomeValue() 
{    
    int popularity, size;
    
    printf("Enter popularity:");
    scanf("%d", &popularity);

    printf("Enter size: ");
    scanf("%d", &size);
    
    homeValue = (pow(popularity, 3) + pow(size, 2)) * 10000;

    printf("Home value is: %d\n", homeValue);
}

int main() 
{
    computeHomeValue();
    
    return 0;
}